			</div>
		</div>

		<div class="container">
			{% set alertTypeAvaillable = [ 'info', 'success', 'warning', 'error'] %}
			{% for alert in alertTypeAvaillable %}
				{% if app.session.getFlashBag.has(alert) %}
					<div class="alert alert-{{ alert }}" >
						<button class="close" data-dismiss="alert">Ã—</button>
						{{ app.session.getFlashBag.get(alert)|trans }}
					</div>
				{% endif %}
			{% endfor %}
		</div>

		{% block body %}
		{% endblock %}

		<div id="footer" class="row">
		<p> meow</p>
		</div>

		{% block javascripts %}

			<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key={{ google.api_key }}&sensor=true">
    </script>
			<script src="{{asset('/assets/js/map.js') }}"></script>
		{% endblock %}
	</div>
