	<div id="wrapper" class="container">
		<div id="breadcrums" class="row">
			meow
		</div>
		<div id="banner" class="row">
			<h1>Buffalo Open Device Lab</h1>

		</div>
		<div id="navigation" class="row">
			<div id="about_us" class="span4">
			</div>
			<div id="devices" class="span4">
				<a href="#"><img src="{{ asset('/assets/img/modbile-overlay.png') }}"></a>
			
			</div>
